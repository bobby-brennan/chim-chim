{
  "manifest_version": 2,

  "name": "SpeedHoles",
  "description": "They make the internet go faster",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },

  "permissions": [
    "storage"
  ],

  "content_scripts": [{
      "matches": ["http://*/*", "https://*/*"],
      "js": ["jquery.js", "speed-holes.js", "speed-holes-runner.js"],
      "run_at": "document_end"
  }]
}
