{
  "manifest_version": 2,

  "name": "Chim-Chim",
  "description": "Go faster",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },

  "content_scripts": [{
      "matches": ["http://*/*"],
      "js": ["jquery.js", "prefetch.js"],
      "run_at": "document_idle"
  }]
}
